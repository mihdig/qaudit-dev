#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/pokrasote/Dev/QA/qaudit-dev/node_modules/.pnpm/@sveltejs+kit@2.49.2_@sveltejs+vite-plugin-svelte@5.1.1_svelte@5.46.0_vite@6.4.1_jiti@2_d1f4aa9128f81e5950d5d7e9ea1f6276/node_modules/@sveltejs/kit/node_modules:/Users/pokrasote/Dev/QA/qaudit-dev/node_modules/.pnpm/@sveltejs+kit@2.49.2_@sveltejs+vite-plugin-svelte@5.1.1_svelte@5.46.0_vite@6.4.1_jiti@2_d1f4aa9128f81e5950d5d7e9ea1f6276/node_modules/@sveltejs/node_modules:/Users/pokrasote/Dev/QA/qaudit-dev/node_modules/.pnpm/@sveltejs+kit@2.49.2_@sveltejs+vite-plugin-svelte@5.1.1_svelte@5.46.0_vite@6.4.1_jiti@2_d1f4aa9128f81e5950d5d7e9ea1f6276/node_modules:/Users/pokrasote/Dev/QA/qaudit-dev/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/pokrasote/Dev/QA/qaudit-dev/node_modules/.pnpm/@sveltejs+kit@2.49.2_@sveltejs+vite-plugin-svelte@5.1.1_svelte@5.46.0_vite@6.4.1_jiti@2_d1f4aa9128f81e5950d5d7e9ea1f6276/node_modules/@sveltejs/kit/node_modules:/Users/pokrasote/Dev/QA/qaudit-dev/node_modules/.pnpm/@sveltejs+kit@2.49.2_@sveltejs+vite-plugin-svelte@5.1.1_svelte@5.46.0_vite@6.4.1_jiti@2_d1f4aa9128f81e5950d5d7e9ea1f6276/node_modules/@sveltejs/node_modules:/Users/pokrasote/Dev/QA/qaudit-dev/node_modules/.pnpm/@sveltejs+kit@2.49.2_@sveltejs+vite-plugin-svelte@5.1.1_svelte@5.46.0_vite@6.4.1_jiti@2_d1f4aa9128f81e5950d5d7e9ea1f6276/node_modules:/Users/pokrasote/Dev/QA/qaudit-dev/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../@sveltejs/kit/svelte-kit.js" "$@"
else
  exec node  "$basedir/../@sveltejs/kit/svelte-kit.js" "$@"
fi
